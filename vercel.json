{
  "version": 2,
  "name": "petgram-devrioma",
  "builds": [
      {
          "use": "@versel/static-build",
          "src": "package.json"
      }
  ],
  "routes": [
      {
          "src": "(.*).js",
          "dest": "/$1.js"
      },
      {
          "src": "(.*).json",
          "dest": "/$1.json"
      },
      {
          "src": "/.*",
          "dest": "index.html"
      }
  ]
}